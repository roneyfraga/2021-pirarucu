@ARTICLE{Troncoso2017613,
author={Troncoso, O.P. and Gigos, F. and Torres, F.G.},
title={Mineral and water content of A. gigas scales determine local micromechanical properties and energy dissipation mechanisms},
journal={Mechanics of Time-Dependent Materials},
