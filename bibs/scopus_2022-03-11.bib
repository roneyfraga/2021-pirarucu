@ARTICLE{Fazzi-Gomes2017,
author={Fazzi-Gomes, P.F. and Melo, N. and Palheta, G. and Guerreiro, S. and Amador, M. and Ribeiro-Dos-santos, A.K. and Santos, S. and Hamoy, I.},
