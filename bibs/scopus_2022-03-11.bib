@ARTICLE{Lima20214227,
author={Lima, A.F. and Rodrigues, A.P.O. and Costa, V.E.},
title={Frozen zooplankton is efficient as natural food during pirarucu Arapaima gigas weaning},
journal={Aquaculture Research},
year={2021},
