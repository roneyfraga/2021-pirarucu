@ARTICLE{Arola2018319,
author={Arola, D. and Murcia, S. and Stossel, M. and Pahuja, R. and Linley, T. and Devaraj, A. and Ramulu, M. and Ossa, E.A. and Wang, J.},
title={The limiting layer of fish scales: Structure and properties},
journal={Acta Biomaterialia},
year={2018},
volume={67},
pages={319-330},
