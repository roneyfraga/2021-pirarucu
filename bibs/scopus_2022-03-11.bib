@ARTICLE{Vieira2018,
author={Vieira, E.C.S. and Mársico, E.T. and Conte-Junior, C.A. and Damiani, C. and Canto, A.C.V.D.C.S. and Monteiro, M.L.G. and Silva, F.A.D.},
title={Effects of different frying techniques on the color, fatty acid profile, and lipid oxidation of Arapaima gigas},
journal={Journal of Food Processing and Preservation},
year={2018},
