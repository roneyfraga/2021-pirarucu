@ARTICLE{Lima2021207,
author={Lima, J.C. and Schorer, M. and Melo, J.F.B. and Braga, L.G.T.},
title={Effect of enzymatic complex in the diet of pirarucu, arapaima gigas juveniles [Efeito de complexo enzimático na dieta para juvenis de pirarucu, arapaima gigas]},
journal={Acta Amazonica},
year={2021},
