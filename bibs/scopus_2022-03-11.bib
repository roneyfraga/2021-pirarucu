@ARTICLE{Andrade-Porto20181015,
author={Andrade-Porto, S.M. and Ramos, C.A. and Roque, R. and Affonso, E.G. and Barcellos, J.F.M. and Queiroz, M.N. and Araújo, C.S.O. and Tavares-Dias, M.},
title={Histopathological evaluation of formalin toxicity in Arapaima gigas (Arapaimidae), the giant fish from Amazon [Avaliação histopatológica da toxicidade da formalina em Arapaima gigas (Arapaimidae), o peixe gigante da Amazônia]},
