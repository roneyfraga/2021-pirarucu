@ARTICLE{Maciel201757,
author={Maciel, P.O. and Muniz, C.R. and Alves, R.R.},
title={Eggs hatching and oncomiracidia lifespan of Dawestrema cycloancistrium, a monogenean parasitic on Arapaima gigas},
