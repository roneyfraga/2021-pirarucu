@ARTICLE{Toledo20191129,
author={Toledo, O.E. and Castañeda, A.E. and Feria, M.A. and Masías, P. and Cueva, M. and Motte, E.},
title={Characterization of the gene sequence of the peptide transport protein (PepT1) in paiche Arapaima gigas fingerlings [Caracterización de la secuencia del gen de la proteín transportadora de péptidos (PepT1) en alevines de paiche Arapaima gigas]},
journal={Revista de Investigaciones Veterinarias del Peru},
year={2019},
