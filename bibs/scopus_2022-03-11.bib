@CONFERENCE{Bourke2016,
author={Bourke, J. and Udrea, B. and Nayak, M.},
title={Pirarucu: The Mars moon prospector},
journal={IEEE Aerospace Conference Proceedings},
year={2016},
volume={2016-June},
doi={10.1109/AERO.2016.7500686},
art_number={7500686},
note={cited By 0; Conference of 2016 IEEE Aerospace Conference, AERO 2016 ; Conference Date: 5 March 2016 Through 12 March 2016;  Conference Code:122440},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84978505410&doi=10.1109%2fAERO.2016.7500686&partnerID=40&md5=8e3b6aee1bbf13e778b97915e9cc7b0e},
affiliation={Embry-Riddle Aeronautical University, Daytona Beach, FL  32114, United States; RedSky Research, Albuquerque, NM  87117, United States},
abstract={The design of the Pirarucu mission has been undertaken by a team of undergraduates students at the Embry-Riddle Aeronautical University for the NASA/NIA 2015 Revolutionary Aerospace Systems Concepts - Academic Linkage (RASC-AL) competition. The team has completed a design iteration up to the equivalent of a mission concept review (MCR). Pirarucu is a Discovery class mission for prospecting Martian moons Phobos and Deimos. The concept is centered on a LADEE-like mothership that carries a set of twelve 12U Cube-Sats. The mothership is equipped with a suite of instruments similar to those on the Curiosity and the 2020 Mars rover, and is capable of performing laser spectroscopy, geological mapping, and chemical analysis of regolith samples. The mothership also serves as a telecommunication hub with the NASA Deep Space Network (DSN) and communicates directly with the CubeSats on a common data link (CDL) architecture implemented with L3 Communications Net-T technologies. The 12U CubeSats explore the surface of the moon and collect surface samples to be returned to the mothership for analysis. The CubeSats are split into two teams, one team can perform in-situ surface science and the other retrieves samples for analysis on the mothership. Both CubeSat teams can be refueled by the mothership and thus are able to perform multiple excursions to the moon for surface science and sample collection to ensure analysis of a variety of sites on the surfaces of the moons. © 2016 IEEE.},
keywords={Laser spectroscopy;  Manned space flight;  Moon;  NASA, Academic linkages;  Aerospace systems;  Deep space networks;  Design iteration;  Geological mapping;  L-3 communications;  Regolith samples;  Sample collection, Chemical analysis},
