@article{ WOS:000257984300013,
Author = {Santos, Claudia Portes and Moravec, Frantisek and Venturieri, Rossana},
Title = {Capillostrongyloides arapaimae sp n. (Nematoda : Capillariidae), a new
   intestinal parasite of the arapaima Arapaima gigas from the Brazilian
   Amazon},
Journal = {MEMORIAS DO INSTITUTO OSWALDO CRUZ},
Year = {2008},
Volume = {103},
Number = {4},
Pages = {392-395},
Month = {JUN},
Abstract = {A new nematode species, Capillostrongyloides arapaimae sp. n., is
   described from the intestine and pyloric caeca of the arapaima, Arapaima
   gigas ( Schinz), from the Mexiana Island, Amazon river delta, Brazil. It
   is characterized mainly by the length of the spicule ( 779- 1,800 mu m),
   the large size of the body (males and gravid females 9.39-21.25 and
   13.54-27.70 mm long, respectively) and by the markedly broad caudal
   lateral lobes in the male. It is the third species of genus
   Capillostrongyloides reported to parasitize Neotropical freshwater
   fishes.},
