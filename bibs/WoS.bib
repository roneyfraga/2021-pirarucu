@article{ WOS:000277820400005,
Author = {Groff, Aline Aparecida and da Silva, Juliana and Nunes, Emilene A. and
   Ianistcki, Martus and Guecheva, Temenouga N. and de Oliveira, Alzira
   Miranda and Feitosa de Oliveira, Christiane Patricia and Val, Adalberto
   Luis and Henriques, Joao A. P.},
Title = {UVA/UVB-induced genotoxicity and lesion repair in Colossoma macropomum
   and Arapaima gigas Amazonian fish},
Journal = {JOURNAL OF PHOTOCHEMISTRY AND PHOTOBIOLOGY B-BIOLOGY},
Year = {2010},
Volume = {99},
Number = {2},
Pages = {93-99},
Month = {MAY 3},
Abstract = {Ultraviolet radiation is known to cause adverse effects to aquatic
   species and aquatic environments. The fish Colossoma macropomum
   (tambaqui) and Arapaima gigas (pirarucu) live in the Amazon basin, near
   the Equator, and thus receive high intensity of ultraviolet radiation.
   Deforestation further aggravates the situation by reducing shade at
   ground level. The aim of this study was to evaluate the genotoxic
   effects of UVA and UVB radiation on erythrocytes of tambaqui and
   pirarucu fish using Micronuclei test and Comet assay. Our study showed
   that UV radiation caused DNA damage in both species as detected by Comet
   assay. In addition, there were differences in response to genotoxicity
   between both species, which are possibly related to their evolutionary
   history. Tambaqui fish exposed to ultraviolet radiation for different
   periods presented clear dose-response in DNA damage profile. Significant
   damage repair was observed 24 h after cessation of ultraviolet radiation
   exposure. At the test conditions used, no significant increase in
   micronucleated cells was observed in tambaqui and pirarucu fish.
   Tambaqui proved to be more sensitive to ultraviolet radiation than
   Pirarucu, as detected by Comet assay, showing statistically higher
   baseline DNA damage. The present results demonstrated that alkaline
   Comet assay was very sensitive for detecting the UV-induced genotoxicity
   during the short exposure period in our study. In addition, the present
   study also suggests that tambaqui and pirarucu fish are useful sentinel
   organisms, as their UV sensitivity allows them to be effective monitors
   of biological hazards in the Amazon region. (C) 2010 Elsevier B.V. All
   rights reserved.},
