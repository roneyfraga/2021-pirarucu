@article{ WOS:000359312600012,
Author = {Cutrim Souza, Rosalia Furtado and Romao Junior, Joao Gomes and Fonseca,
   Adriana Figueiredo and Luz, Ronald Kennedy and Takata, Rodrigo},
Title = {Feed conditioning periods for pirarucu juveniles in the feeding
   transition from moist to dry feed},
Journal = {PESQUISA AGROPECUARIA BRASILEIRA},
Year = {2015},
Volume = {50},
Number = {7},
Pages = {622-625},
Month = {JUL},
Abstract = {The objective of this work was to evaluate the effect of feed training
   periods on the productive performance and survivorship of pirarucu
   juveniles (Arapaima gigas), during the transition from ground fish mass
   diet to a dry-feed diet. Animals with 15.8+/- 1.2 g were evaluated with
   gradual substitutions of the diet at every 2, 3, 4, and 5 days. After 12
   days from feed training, the substitutions at every two and three days
   provided the highest specific growth rates. The replacement of diet
   based on forage fish by dry formulated diet may be done every two or
   three days.},
