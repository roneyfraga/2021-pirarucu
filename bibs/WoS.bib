@inproceedings{ WOS:000477977800200,
Author = {Barretto, T. L. and Betiol, L. F. L. and Bellucci, E. R. B. and
   Telis-Romero, J. and Barretto, A. C.},
Editor = {Carcel, JA and Clemente, G and GarciaPerez, JV and Mulet, A and Rossello, C},
Title = {Salting kinetics, salt diffusivities and proximate composition in
   osmotically dehydrated Pirarucu muscle},
