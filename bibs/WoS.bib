@article{ WOS:000234832100009,
Author = {Crescencio, R and Ituassu, DR and Roubach, R and Pereira, M and Cavero,
   BAS and Gandra, AL},
Title = {Influence of feeding period on consumption and weight gain of pirarucu},
Journal = {PESQUISA AGROPECUARIA BRASILEIRA},
