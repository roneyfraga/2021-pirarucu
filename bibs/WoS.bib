@article{ WOS:000699985300004,
Author = {Lima, Julliana de Castro and Schorer, Marianne and Bibiano Melo, Jose
   Fernando and Tavares Braga, Luis Gustavo},
Title = {Effect of enzymatic complex in the diet of pirarucu, Arapaima gigas
   juveniles},
Journal = {ACTA AMAZONICA},
Year = {2021},
Volume = {51},
Number = {3},
Pages = {207-213},
Month = {JUL-SEP},
Abstract = {The pirarucu, Arapaima gigas is a native, carnivorous fish species from
   the Amazon basin. As carnivorous fish have low amylase activity,
   exogenous enzymes can improve the digestibility of carbohydrates in
   aquaculture feeds. We evaluated the digestibility of increasing levels
   of an enzymatic complex in diets of pirarucu juveniles (65.2 +/- 0.4 g).
   The experimental design was randomized with four treatments {[}diets
   containing 0.25, 0.50, 0.75, and 1 g kg(-1) on-top inclusion of an
   enzyme complex (Allzyme (R) SSF (R), USA), and a control, with three
   replications at a density of 5 fish per unit, and a 30-day duration. We
   quantified apparent digestibility of dry matter, crude protein, and
   crude energy through nutrient and chromium oxide content in diets and
   feces. Enzymatic activity, hepatic glycogen and total protein were
   determined in liver and anterior intestine samples. The diet with 1 g
   kg(-1) of enzyme-complex resulted in an increase in apparent
   digestibility of crude protein, gross energy, and dry matter, hepatic
   glycogen, total proteins in liver and in intestine, showing the
   efficiency of the enzyme complex in pirarucu feeding. A higher
   accumulation of dry matter, crude energy, and ethereal extract in the
   carcass indicated weight increase in the fish treated with enzymatic
   complex. A decrease in the endogenous enzymatic activity (protease,
   lipase and amylase) suggested an improved efficacy of the digestive
   process. Our results indicate that the inclusion of 1 g kg(-1) enzyme
   complex in the diet of juvenile pirarucu can be recommended to achieve
   greater digestibility of nutrients and improvement in productive
   performance.},
