@article{ WOS:000291326600016,
Author = {de Castro Canani, Luis Gustavo and Menezes, Mariangela and Torgan,
   Lezilda Carvalho},
Title = {Epilithic diatoms from acidic and oligotrophic waters in Southeast
   Brazil},
