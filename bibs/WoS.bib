@article{ WOS:000446102700017,
Author = {Vialle, Ricardo Assuncao and Santana de Souza, Jorge Estefano and Lopes,
   Katia de Paiva and Teixeira, Diego Gomes and Alves Sobrinho, Pitagoras
   de Azevedo and Ribeiro-dos-Santos, Andre M. and Furtado, Carolina and
   Sakamoto, Tetsu and Oliveira Silva, Fabio Augusto and Correa de
   Oliveira, Edivaldo Herculano and Hamoy, Igor Guerreiro and Assumpcao,
   Paulo Pimentel and Ribeiro-dos-Santos, Andrea and Matos Santos Lima,
   Joao Paulo and Seuanez, Hector N. and de Souza, Sandro Jose and Santos,
   Sidney},
Title = {Whole Genome Sequencing of the Pirarucu (Arapaima gigas) Supports
   Independent Emergence of Major Teleost Clades},
Journal = {GENOME BIOLOGY AND EVOLUTION},
Year = {2018},
Volume = {10},
Number = {9},
Pages = {2366-2379},
Month = {SEP},
Abstract = {The Pirarucu (Arapaima gigas) is one of the world's largest freshwater
   fishes and member of the superorder Osteoglossomorpha (bonytongues), one
   of the oldest lineages of ray-finned fishes. This species is an obligate
   air-breather found in the basin of the Amazon River with an attractive
   potential for aquaculture. Its phylogenetic position among bony fishes
   makes the Pirarucu a relevant subject for evolutionary studies of early
   teleost diversification. Here, we present, for the first time, a draft
   genome version of the A. gigas genome, providing useful information for
   further functional and evolutionary studies. The A. gigas genome was
   assembled with 103-Gb raw reads sequenced in an Illumina platform. The
   final draft genome assembly was similar to 661 Mb, with a contig N50
   equal to 51.23 kb and scaffold N50 of 668 kb. Repeat sequences accounted
   for 21.69\% of the whole genome, and a total of 24,655 protein-coding
   genes were predicted from the genome assembly, with an average of nine
   exons per gene. Phylogenomic analysis based on 24 fish species supported
