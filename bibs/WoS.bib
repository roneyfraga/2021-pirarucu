@article{ WOS:000416021500021,
Author = {Martins, Mayara Galvao and Galvao Martins, Deborah Elena and Pena,
   Rosinelson da Silva},
Title = {Chemical composition of different muscle zones in pirarucu (Arapaima
   gigas)},
Journal = {FOOD SCIENCE AND TECHNOLOGY},
