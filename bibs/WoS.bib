@article{ WOS:000342887200008,
Author = {Porto Braga, Tony Marcos and Henrique Rebelo, George},
Title = {TRADITIONAL KNOWLEDGE OF THE FISHERMEN OF THE LOWER JURUA RIVER: ASPECTS
   RELATED TO THE FEEDING HABITS OF FISH IN THE REGION},
