@article{ WOS:000353629200017,
Author = {Farias, Izeni Pires and Leao, Adam and Almeida, Yane Santos and Verba,
   Julia Tovar and Marcelo Crossa, M. and Honczaryk, Alexandre and Hrbek,
   Tomas},
Title = {Evidence of polygamy in the socially monogamous Amazonian fish Arapaima
   gigas (Schinz, 1822) (Osteoglossiformes, Arapaimidae)},
Journal = {NEOTROPICAL ICHTHYOLOGY},
Year = {2015},
Volume = {13},
Number = {1},
Pages = {195-203},
Month = {JAN-MAR},
Abstract = {Arapaima gigas is one of the largest freshwater fishes of the world. It
   is socially monogamous, forming pairs, constructing a nest and providing
   parental care. We performed a paternity analysis under three scenarios
   in captive, semi-natural and natural areas using 10 microsatellite
   markers. As a positive control, we analyzed three pairs and their
   offspring isolated individually in artificial breeding ponds (a priori
   very high probability of monogamy). We then analyzed two samples of
   offspring from large artificial ponds with multiple adults but only one
   reproductive pair (a priori high probability of monogamy), two samples
   from semi-natural breeding station with multiple adults but only one
   reproductive pair (a priori high probability of monogamy), and a sample
   from a natural lake with multiple adults, some potentially breeding (a
   priori medium probability of monogamy). Analysis of patterns of
   Mendelian heredity suggested an extra-pair contribution for all broods
   except the positive controls. Similarly, results based on multilocus
   analysis estimated at least two sib-groups per nest. These results
   reject monogamy as a system of breeding in Arapaima gigas. From a
   management perspective, this behavior may be exploited to maintain
   genetic diversity in captive and as well in wild populations of Arapaima
   gigas.},
Publisher = {SOC BRASILEIRA ICTIOLOGIA},
Address = {UNIV SAO PAULO, DEPT FISIOLOGIA-IB, RUA DO MATAO, TRAVESSA 14 N 321, SAO
   PAULO, SP 05508-900, BRAZIL},
Type = {Article},
Language = {English},
Affiliation = {Farias, IP (Corresponding Author), Univ Fed Amazonas, Inst Ciencias Biol, Dept Genet, Lab Evolucao \& Genet Anim, Av Gen Rodrigo Octavio Jordao Ramos 3000, BR-69077000 Manaus, Amazonas, Brazil.
   Farias, Izeni Pires; Leao, Adam; Verba, Julia Tovar; Hrbek, Tomas, Univ Fed Amazonas, Inst Ciencias Biol, Dept Genet, Lab Evolucao \& Genet Anim, BR-69077000 Manaus, Amazonas, Brazil.
   Almeida, Yane Santos, Fac Integradas Tapajos, BR-68010200 Santarem, PA, Brazil.
