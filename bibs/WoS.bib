@article{ WOS:000274443400012,
Author = {Gonzalez, R. J. and Brauner, C. J. and Wang, Y. X. and Richards, J. G.
   and Patrick, M. L. and Xi, W. and Matey, V. and Val, A. L.},
Title = {Impact of Ontogenetic Changes in Branchial Morphology on Gill Function
   in Arapaima gigas},
Journal = {PHYSIOLOGICAL AND BIOCHEMICAL ZOOLOGY},
Year = {2010},
Volume = {83},
